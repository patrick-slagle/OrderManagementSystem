<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>OrderManagementSystem</display-name>
  <servlet>
    <servlet-name>CookieStore</servlet-name>
    <servlet-class>org.patrickslagle.CookieStore</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CookieStore</servlet-name>
    <url-pattern>/checkcookie.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoginValidationServlet</servlet-name>
    <servlet-class>org.patrickslagle.controller.LoginValidationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginValidationServlet</servlet-name>
    <url-pattern>/homepage.do</url-pattern>
  </servlet-mapping>
</web-app>